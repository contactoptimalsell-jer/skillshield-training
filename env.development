# Development Environment Variables
# Copy this to .env and fill with your real values

# Supabase Configuration (REQUIRED)
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key

# Stripe Configuration (REQUIRED)
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_your-publishable-key
STRIPE_SECRET_KEY=sk_test_your-secret-key
STRIPE_WEBHOOK_SECRET=whsec_your-webhook-secret

# OpenAI Configuration (REQUIRED)
OPENAI_API_KEY=sk-your-openai-key

# App Configuration
VITE_APP_URL=http://localhost:5173

# Stripe Price IDs (REQUIRED - Get from Stripe Dashboard)
STRIPE_BOUCLIER_PRICE_ID=price_your-bouclier-price-id
STRIPE_FORTERESSE_PRICE_ID=price_your-forteresse-price-id

# Development Mode
NODE_ENV=development

